rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    
    // TEMPORARY RULES FOR DATA INITIALIZATION
    // These rules allow anyone to read/write for initial setup
    // DO NOT use these rules in production!
    
    // Allow all operations during initialization
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
